[color]
  branch = auto
  diff = auto
  status = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[user]
	name = Adrien Maillol
	email = maillol.adrien@gmail.com
[alias]
  d = diff
  dc = diff --cached
  l = log
  ll = log --pretty=oneline --abbrev-commit --decorate=full --graph
  la = log --pretty=format:'%C(yellow)%h%C(red)%d%Creset %Creset %s - %C(blue)%ae %C(cyan) Authored : %ar %C(white) - %C(green) Commiter :  %cn, %cr' --decorate=full --graph
  ls = log --stat --decorate
  lp = log -p
  s = status
  suno = status -uno
  omi = push --force
  co = checkout
  newremote = ! git push origin origin:refs/heads/$1
  deleteremote = push origin :refs/heads/
[core]
	editor = vim

